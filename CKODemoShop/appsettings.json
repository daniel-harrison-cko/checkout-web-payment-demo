{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level:u3}] {Message:lj}{NewLine}{Properties}{NewLine}{Exception}",
          "restrictedToMinimumLevel": "Warning"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "Properties": {
      "ApplicationName": "Web.Payment.Demo"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "ElasticSearch": {
    "Node": "http://localhost:9200/",
    "AutoRegisterTemplate": true,
    "IndexFormat": "demo-{0:yyyy.MM.dd}"
  },
  "AllowedHosts": "*",
  "Okta": 
  {
    "OktaDomain": "https://dev-320726.okta.com/"
  },
  "OktaWebClient": 
  {
    "Issuer": "https://dev-320726.okta.com/oauth2/default",
    "ClientId": "0oa11suy0u5Ec7Lg9357"
  },
  "CheckoutApiOptions": {
    "SecretKey": "secret-key",
    "PublicKey": "public-key",
    "GatewayUri": "https://gateway-uri.com"
  }
}